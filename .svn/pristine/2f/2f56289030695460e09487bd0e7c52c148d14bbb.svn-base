.title-line{
  margin: 20px 150px;
}
.shop-info-content,.facility-content{
  padding: 10px 150px;
}
.block{
  display: block;
  /* margin-top: 20px;*/
}
label{
  width: 105px;
  display: inline-block;
  text-align: right;
}
.line-name{
  display: inline-block;
  text-align: right;
  margin-right: 10px;
}
.divider{
  margin:0px 5px;
}
.required-tag{
  color: red;
}
.not-finished,.light-weight-grey{
  color:#898990;
}
.model-popup,.add-item,.edit-info{
  color: #467ac1;
  text-decoration: underline;
  cursor: pointer;
}
.inline-input{
  display: inline-block;
  width:160px;
}
.input-style{
  width: 160px;
}
.select-style{
  display: inline-block;
  width: 160px;
}

.nav-tabs .nav-item .nav-link{
  color: #495057;
}

.add-popup-btn{
  width: 220px;
}
.selected-span{
  color: red;
}

.inline-width-240{
  width: 240px;
}
/*店名*/
.brand-name{
  color: red;
}
.input-label-name{
  font-size: 16px;
  line-height: 38px;
}
.shop-name-input,.brand-name-input,.subshop-name{
  display: inline-block;
  width: 10%;
}
.checkbox-style{
  display: inline-block;
  margin-left: 15px;
}

.brand-name-block{
  margin-top: 10px;
}
/*经营行业*/
#big-industry,#small-industry{
  display: inline-block;
  width: 160px;
}

/*营业时间*/
.tab-block{
  display: inline-block;
  width: 50%;
  vertical-align: middle;
}
.week-list{
  padding: 10px 0px;
}
.week-item{
  margin:0px 8px;
}
.week-checkbox{
  margin-right: 3px;
}
.time-input{
  padding-left: 10px;
  margin:0px 5px;
}

/*提供服务*/
.service-list{
  display: inline-block;
  width: 300px;
  padding: 8px 0px;
}
.service-item{
  margin:0px 8px;
  display: inline-block;
  width: 80px;
}
/*店铺租金*/
.rent-input{
  width: 20%;
  display: inline-block;
}
/*支付方式*/
.pay-way-list{
  display: inline-block;
  width: 80%;
}
.pay-way-item{
  padding: 10px 0px;
  margin:0px 10px;
}

/*租金弹出框*/
.rate{
  display: inline-block;
}
.select-radio{
  margin-right: 3px;
}
.select-span{
  /* padding: 10px 0px;*/
  margin:10px 10px;
  display: inline-block;
}

/*营业额*/
.profit{
  margin-right: 10px;
}

textarea{
  vertical-align: middle;
}

/*证件照*/
.photos-list{
  display: inline-block;
}

/*物业建筑信息*/
.left-inline-block,.right-inline-block{
  display: inline-block;
  width: 350px;
}
/*适合经营*/
.span-list-item{
  width: 75px;
  display: inline-block;
}
.span-list-item i{
  margin-left: 10px;
}

/*物业配套*/
.facilities-list{
  display: inline-block;
  width: 500px;
  padding: 8px 0px;
}
.facility-item{
  display: inline-block;
  width: 100px;
}
/*费用成本*/
.fee-left-inline-block,.fee-right-inline-block{
  display: inline-block;
}
.fee-left-inline-block label,.fee-right-inline-block label{
  width: 50px;
}

/*店铺证件*/
.shop-certifications{
  display: inline-block;
  list-style: none;
  margin: 0px;
  padding: 0px;
  padding-top: 6px;
}
.shop-certifications li{
  display: block;
}
.add-certifications-btn{
  margin-top: 5px;
}
.certification-list{
  display: inline-block;
  vertical-align: middle;
}

.upload-block-inline{
  display: inline-block;
  text-align: center;
  width: 80px;
}

/*行业选择弹出框*/
.popup-industry-list{
  list-style: none;
}
.popup-industry-list li{
  padding-left: 80px;
  margin-top: 15px;
}
.big-industry-style{
  display: inline-block;
  width: 100px;
  text-align: right;
  margin-left: -100px;
}
.small-industry-style{
  display: inline-block;
  margin:0px 10px;
}

/*个人信息弹出框*/

/*行业列表弹出框*/

.industry-selected-span{
  border: 1px solid black;
  margin-left: 10px;
  padding: 0px 5px;
}
.industry-selected-span i{
  margin-left: 10px;
}
/*店铺图片*/
.upload-block{
  display: inline-block;
}
.old-images-block{
  display: inline-block;
  width: 100px;
  position: relative;
}
.new-image-block{
  display: inline-block;
  vertical-align: middle;
  width: 45px;
  height: 48px;
  overflow: hidden;
}
.upload-btn{
  line-height: 55px;
  margin-top: -3px;
  margin-left: -3px;
  font-size: 50px;
  width: 50px;
  height: 50px;
  color: #a9a9a9;
}
.img-item-box img{
  width: 100px;
}

.upload-block i{
  position: absolute;
  top: 0px;
  right: 5px;
  font-size: 20px;
}
.release-btn{
  display: block;
  margin: 0 auto;
}

/*baidu-map*/
.baidu-map{
  width: 450px;
  height: 320px;
  display: block;

  position: absolute;
  top: 0px;
  right: 10%;
}
.baidu-map-cancel{
  position: absolute;
  top: 0px;
  right: 5px;
  font-size: 20px;
}

.map-btn-control{
  display: inline-block;
  width: 20%;
  margin-left: 20%;
  margin-top: 15px;
}


